{
  "name": "next-pkg-pkg",
  "version": "1.0.0",
  "description": "demo",
  "private": true,
  "scripts": {
    "clean": "rm -rf build dist out .tscache",
    "move": "cp -r ./node_modules/next-pkg-web/.next ./out && cp ./build/server.js ./out/standalone",
    "build": "yarn clean && yarn build:ts && yarn move && yarn build:pkg",
    "build:ts": "tsc --build .",
    "build:pkg": "pkg out/standalone --public --public-packages \"*\" --no-bytecode --compress=GZip"
  },
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "next-pkg-web": "1.0.0",
    "pkg": "^5.6.0"
  },
  "devDependencies": {
    "@types/express": "^4",
    "@types/node": "^17.0.23",
    "typescript": "^4.6.3"
  },
  "pkg": {
    "assets": [
      "out/**/*.*",
      "out/**/*"
    ],
    "assets-----removed": [
      "node_modules/**/*.*"
    ],
    "targets": [
      "node16-macos-arm64"
    ],
    "outputPath": "public"
  }
}
