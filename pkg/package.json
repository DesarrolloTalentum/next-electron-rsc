{
  "name": "next-pkg-pkg",
  "version": "1.0.0",
  "description": "demo",
  "bin": "build/index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf build dist out .tscache",
    "build": "yarn clean && yarn build:ts && yarn build:pkg",
    "build:ts": "tsc --build .",
    "build:pkg": "pkg . --public --public-packages \"*\" --no-bytecode --compress=GZip"
  },
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.1",
    "next-pkg-web": "1.0.0",
    "pkg": "^5.8.1",
    "resolve": "^1.22.8"
  },
  "devDependencies": {
    "@types/express": "^4",
    "@types/node": "^22.7.6",
    "@types/resolve": "^1",
    "typescript": "^5.6.3"
  },
  "pkg": {
    "assets": [
      "node_modules/next-pkg-web/.next/**/*.*",
      "node_modules/next-pkg-web/.next/**/*"
    ],
    "targets": [
      "node18-macos-arm64"
    ],
    "outputPath": "out"
  }
}
